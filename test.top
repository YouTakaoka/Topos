Int x = 4
Int y = 5
print ( x * y == 20 )
Func foo = & x -> x * x
print ( foo 4 == 16 )
Func bar = & x y -> ( x + y ) / 2
print ( bar 3 5 == 4.0 )
print ( foo ( foo 4 ) == 256 )
Func f = & g z -> g ( g z )
print ( f foo 4 == 256 )

print "Hello, world!"

Func fact = & x -> if x > 0 then x * ( fact ( x - 1 ) ) else 1
print ( fact 4 + 1 == 25 )
print ( 1 + fact 4 == 25 )
print ( f succ 4 == 6 )
print ( succ ( succ 4 ) == 6 ) # piyopiyo
print ( head [ 1 , 2 , 3 ] == 1 )
let tpl = (( 4 , "hoge" ))
print ( fst tpl == 4 )
print ( snd tpl == "hoge" )
let ls = seq 1 10
print ( pop ls )
let sum = & ls -> if isEmpty ls then 0 else sum ( tail ls ) + head ls
print ( sum ls == 55 )
print ( take 3 ls )
print ( map succ ls )
print ( map foo ls )
print ( map fact ls )
